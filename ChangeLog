	  grep -Ee '\.(hxx|hh|cc|hcc|txt|tig|m4)$' |
	   while read f
	   do
	     test -f $f -a -w $f || continue
	     gexpand $f >$f.t
	     perl -pe 's/ +$//' $f.t > $f
	     rm $f.t
